<div class="container-fluid">
  <div class="row h-100 border border-dark bg-dark text-white">
    <div class="col-2 my-auto">
      <h3>Typ / Baufeld</h3>
      <button type="button" name="button" class="btn btn-primary btn-sm" ng-click="home.resetFilter()">Reset Filter</button>
    </div>
    <div class="col-10">
      <div class="row">
        <div ng-repeat="(key, value) in home.data" class="col text-center my-auto" ng-if="key != 'total'">
