              <!-- {{home.subnetModel}} - {{home.subnetResults}} - -->
              <!-- {{home.radioModelSubnet}} -->
            </div>

            <div class="btn-group .d-none.d-md-block">
              <label class="btn btn-secondary border border-dark" ng-model="home.radioModelSubnet[key]" uib-btn-radio="value.meta_data.floor" uncheckable><span class="fa fa-th-large fa"></span> <small>{{value.meta_data.floor | number}} m²</small></label>
              <label class="btn btn-secondary border border-dark" ng-model="home.radioModelSubnet[key]" uib-btn-radio="value.meta_data.occupants" uncheckable><span class="fa fa-user"></span> <small>{{value.meta_data.occupants | number}} </small></label>
              <label class="btn btn-secondary border border-dark" ng-model="home.radioModelSubnet[key]" uib-btn-radio="value.meta_data.apartments" uncheckable><span class="fa fa-home"></span> <small>{{value.meta_data.apartments | number}} </small></label>
            </div>

